@model List<SAP.Web.Areas.Admin.Models.PhaseViewModel>

@{
    for (int i = 0; i < Model.Count; i++)
    {
        <h6>Faza @i</h6>
        <hr />
        <div class="form-group">
            @Html.LabelFor(x => Model[i].Name, new { @class = "control-label col-md-3" })
            <div class="col-md-9">
                @Html.EditorFor(x => Model[i].Name, "", "Phases[" + i + "].Name", new { @class = "form-control" })
                @Html.ValidationMessageFor(x => Model[i].Name, "", new { @class = "text-danger" })
                @Html.ValidationMessage("Phases[" + i + "].Name", new { @class = "text-danger" })
            </div>
        </div>
        <div class="form-group">
            @Html.LabelFor(x => Model[i].MaxUsers, new { @class = "control-label col-md-3" })
            <div class="col-md-9">
                @Html.EditorFor(x => Model[i].MaxUsers, "", "Phases[" + i + "].MaxUsers", new { @class = "form-control" })
                @Html.ValidationMessage("Phases[" + i + "].MaxUsers", new { @class = "text-danger", @name = "Phases[" + i + "].MaxUsers" })
            </div>
        </div>
        <div class="form-group">
            @Html.LabelFor(x => Model[i].Order, new { @class = "control-label col-md-3" })
            <div class="col-md-9">
                @Html.EditorFor(x => Model[i].Order, "", "Phases[" + i + "].Order", new { @class = "form-control", @value = i })
                @Html.ValidationMessage("Phases[" + i + "].Order", new { @class = "text-danger", @name = "Phases[" + i + "].Order" })
            </div>
        </div>
        <div class="form-group">
            @Html.LabelFor(x => Model[i].TaskCount, new { @class = "control-label col-md-3" })
            <div class="col-md-9">
                @Html.EditorFor(x => Model[i].TaskCount, "", "Phases[" + i + "].TaskCount", new { @class = "form-control" })
                @Html.ValidationMessage("Phases[" + i + "].TaskCount", new { @class = "text-danger", @name = "Phases[" + i + "].TaskCount" })
            </div>
        </div>
    }
}
<div class="form-group">
    <div class="col-md-offset-8 col-md-4">
        <button data-toggle="collapse" data-parent="#accordion" href="#collapse1" class="btn btn-default">Cofnij</button>
        <button data-toggle="collapse" data-parent="#accordion" href="#collapse3" class="btn btn-success" onclick="configureTask()">Dalej</button>
    </div>
</div>